{
    "name": "backend-dev",
    "dockerComposeFile": ["../../compose.yaml/", "./compose.yaml"],
    "service": "backend",
	"workspaceFolder": "${localWorkspaceFolder}",
	"runServices": ["backend"],
	"shutdownAction":"none",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"mhutchie.git-graph",
				"donjayamanne.githistory"
			]
		}
	}
}